<h1>Listing Tickets</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Seat id seq</th>
    <th>Address</th>
    <th>Price</th>
    <th>Email address</th>
    <th>Phone</th>
    <th>event_id</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.name %></td>
        <td><%= ticket.seat_id_seq %></td>
        <td><%= ticket.address %></td>
        <td><%= ticket.price %></td>
        <td><%= ticket.email_address %></td>
        <td><%= ticket.phone %></td>
        <td><%= ticket.event_id %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>